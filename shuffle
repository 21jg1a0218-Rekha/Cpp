//To print the shuffle elements
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    vector<int>nums;
    for(int i=0;i<2*n;i++)
    {
        int values;
        cin>>values;
        nums.push_back(values);
    }
    vector<int>x;
    for(int i=0;i<n;i++)
    {
        x.push_back(nums[i]);
        x.push_back(nums[n+i]);
    }
    for(int i=0;i<2*n;i++)
    {
        cout<<x[i]<<" ";
    }

}
