// to print even and odd arrays by using vectors
#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int>arr;
    int n,temp=0;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>temp;
        arr.push_back(temp);
    }
    vector<int>even;
    vector<int>odd;
    for(int i=0;i<n;i++)
    {
        
        if(arr[i]%2==0)
        {
            even.push_back(arr[i]);
        }
        else{
            odd.push_back(arr[i]);
        }
    }
    for(int i=0;i<even.size();i++)
    {
        cout<<""<<even[i];
    }
     for(int i=0;i<odd.size();i++)
    {
        cout<<""<<odd[i];
    }
    
    return 0;
}
